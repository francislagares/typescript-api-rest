generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Car {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  color       String
  gas         Gas
  year        DateTime
  description String
  price       Int
}

enum Gas {
  GASOLINE
  ELECTRIC
}
